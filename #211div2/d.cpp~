using namespace std;
typedef long long ll;
typedef pair<int,int> ppi;
typedef pair<ll,ll> ppl;
#define FOR(a,b,c) for(int a=(b);a<(c);++a)
#define FR(a,b) for(typeof(b.begin()) a=b.begin();a!=b.end();++a)
int main()
{
  int n,m,a;
  scanf("%d%d%d",&b,&n,&a);
  vector<int> b,p;
  b.resize(n);
  FOR(i,0,n)
    scanf("%d",&b[i]);
  sort(b.begin(),b.end());
  p.resize(m);
  FOR(i,0,m)
    scanf("%d",&p[i]);
  
}
